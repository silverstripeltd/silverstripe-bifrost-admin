{
  "compilerOptions": {
    "target": "es6",
    "sourceMap": true,
    "skipLibCheck": true,
    "lib": ["dom", "es2018"],
    "moduleResolution": "Bundler",
    "module": "esnext",
    "jsx": "react",
    "importHelpers": true,
    "plugins": [{ "name": "typescript-plugin-css-modules" }],
  },
  "include": [
    "src"
  ],
  "exclude": [
    "node_modules"
  ]
}
