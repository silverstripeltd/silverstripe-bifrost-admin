{
    "name": "silverstripeltd/silverstripe-bifrost-admin",
    "description": "Silverstripe Search Administration module",
    "type": "silverstripe-vendormodule",
    "keywords": [
        "silverstripe",
        "search",
        "CMS",
        "elastic",
        "service"
    ],
    "license": "BSD-3-Clause",
    "require": {
        "silverstripe/framework": "^6.0",
        "silverstripe/admin": "^3.0",
        "silverstripe/silverstripe-forager-bifrost": "2.0.0"
    },
    "require-dev": {
        "silverstripe/recipe-testing": "^4",
        "slevomat/coding-standard": "~8.18.0",
        "phpstan/extension-installer": "^1.3"
    },
    "autoload": {
        "psr-4": {
            "SilverstripeSearch\\": "src/",
            "SilverstripeSearch\\Tests\\": "tests/php/"
        }
    },
    "extra": {
        "expose": [
            "client/base/dist",
            "client/synonyms/dist",
            "client/landing/dist"
        ],
        "branch-alias": {
            "dev-main": "2.0.x-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "allow-plugins": {
            "composer/installers": true,
            "dealerdirect/phpcodesniffer-composer-installer": true,
            "php-http/discovery": true,
            "phpstan/extension-installer": true,
            "silverstripe/recipe-plugin": true,
            "silverstripe/vendor-plugin": true
        }
    }
}
